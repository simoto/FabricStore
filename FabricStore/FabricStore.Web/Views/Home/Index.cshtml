@model IEnumerable<FabricStore.Web.Models.ProductHomeViewModel>
@{
    ViewBag.Title = "Home Page";
}

<div class="well">
    <h2>New products</h2>
    <div class="row">
        @for (int i = 0; i < Model.Count(); i++)
        {
            var product = Model.ElementAt(i);
            @Html.Partial("_ProductPartial", product)                   
        }
    </div>
</div>